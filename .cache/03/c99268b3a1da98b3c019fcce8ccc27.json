}},{"source":"style.scss","name":null,"original":{"line":147,"column":4},"generated":{"line":1,"column":2047}}],"sources":{"style.scss":"\r\nbody {\r\n    box-sizing: border-box;\r\n    display: flex;\r\n    flex-direction: column;\r\n}\r\n\r\n.main {\r\n    position: relative;\r\n    z-index: -9;\r\n    font-size: 2em;\r\n    margin: 100px 200px;\r\n    @media (max-width:800px) {\r\n        margin:40px 15px;\r\n    }\r\n}\r\n.navbar {\r\n    font-weight: 500;\r\n    background-color: rgba(245, 245, 245, 0.808);\r\n    box-shadow: 0 0px 42px 3px black;\r\n    display: flex;\r\n    position: fixed;\r\n    width: 100%;\r\n    z-index: 20;\r\n\r\n    &__items {\r\n        display: flex;\r\n        list-style: none;\r\n        gap: 10px;\r\n        font-size: 1.5em;\r\n        transform: translateX(0);\r\n        transition: transform 0.5s ease-in;\r\n         li {\r\n             padding: 4px;\r\n             margin:0  5px;\r\n         }\r\n        @media (max-width: 600px) {\r\n            transform: translateX(-500px)\r\n        }\r\n    }\r\n\r\n}\r\n\r\n\r\n\r\n.sidebarnav {\r\n    z-index: 999;\r\n    padding: 25px;\r\n    display: flex;\r\n    align-self: center;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    position: fixed;\r\n    width: 300px;\r\n    margin-top: 300px;\r\n    transform: translateY(-1000px);\r\n    border-radius: 100%;\r\n    transition:transform 0.5s ease-in, background-color 1s ease-in;\r\n    @media screen {\r\n        margin-top:150px\r\n    }\r\n    a, i {\r\n        text-decoration: none;\r\n        font-size: 2em;\r\n        color: white;\r\n        margin:10px;\r\n        @media(max-width:700px) {\r\n            color: red($color: #000000);\r\n        }\r\n        &:hover {\r\n            color: rgb(238, 234, 32);\r\n        }\r\n    }\r\n}\r\n.sidebarnav--active {\r\n    transform: translateX(0px);\r\n    background-color: rgba(0, 0, 0, 0.836);\r\n\r\n}\r\n\r\n\r\n\r\n.hamburger {\r\n    padding: 10px;\r\n    display: inline-block;\r\n    cursor: pointer;\r\n    background-color: transparent;\r\n    border:0;\r\n    margin: 0\r\n}\r\n.hamburger__box {\r\n    width:40px;\r\n    height: 24px;\r\n    display: inline-block;\r\n    position: relative;\r\n}\r\n@mixin hamburger-line {\r\n    width:100%;\r\n    height: 3px;\r\n    position: absolute;\r\n    background-color: black\r\n}\r\n.hamburger__inner {\r\n    @include hamburger-line;\r\n    \r\n        left: 0;\r\n        top: 50%;\r\n        transform: translateY(-50%);\r\n        transition: background-color 0.3s .1s ease-in-out\r\n}\r\n\r\n.hamburger__inner::before,\r\n.hamburger__inner::after\r\n {\r\n    @include hamburger-line;\r\n    content: '';\r\n    left: 0;\r\n    transition: transform 0.3s ease-in-out\r\n}\r\n\r\n.hamburger__inner::after {\r\n\r\n    top: 10px;\r\n}\r\n\r\n.hamburger__inner::before {\r\n\r\n    top: -10px;\r\n}\r\n\r\n.hamburger--active  .hamburger__inner {\r\n    background-color: transparent\r\n}\r\n.hamburger--active .hamburger__inner::before {\r\n     transform:translateY(10px)  rotate(45deg) \r\n\r\n}\r\n.hamburger--active .hamburger__inner::after{\r\n    transform:translateY(-10px) rotate(-45deg) \r\n\r\n}\r\n.hamburger, .nav {\r\ntransition: transform 0.3s 0.1s ease-in-out\r\n\r\n}\r\n.hamburger--active {\r\n    transform: translate(0px)\r\n}"},"lineCount":null}},"error":null,"hash":"75b74c82af9247cbfdf08ce534152602","cacheData":{"env":{}}}